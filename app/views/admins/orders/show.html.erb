<div class="container">
	<h1 class="my-4">管理者用/注文詳細</h1>
	<hr>
		<div class="row">
			<table class="table table-hover">
				<tbody>
					<tr>
						<td>商品ID</td>
						<td>販売者</td>
						<td>購入者</td>
						<td>商品名</td>
						<td>商品URL</td>
						<td>値段</td>
						<td>購入日時</td>
						<td>商品登録日</td>
					</tr>
						<% @order.product_orders.each do |product_order| %>
						<tr>
							<td><%= @order.id %></td>
							<td><%= product_order.product.user.nickname%></td>
							<td><%= @order.user.last_name %><%= @order.user.first_name %></td>
							<td><%= product_order.product.title %></td>
							<td><%= product_order.product.url%></td>
							<td><%= product_order.product.price %>円</td>
							<td><%= @order.created_at.strftime("%m#{"月"}%d#{"日"}%R") %></td>
							<td><%= product_order.product.created_at.strftime("%m#{"月"}%d#{"日"}%R")%></td>
						</tr>
						<% end %>
						<tr>
							<td></td>
							<td></td>
							<td></td>
							<td></td>
							<td><%= link_to "戻る", admins_orders_path, class: "btn btn-default" %></td>
							<td><%= link_to "編集",  edit_admins_order_path(@order.id), class: "btn btn-success" %></td>
							<td><%= link_to "削除", admins_order_path(@order.id), method: :delete, class: "btn btn-danger", data: { confirm: '削除します。'} %></td>
						</tr>
				</tbody>
			</table>
		</div>
	</div>
</div>



