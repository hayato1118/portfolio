                            <div class="">
                                <div class="row">
                                    <div class="col-md-6 form_text">
                                        <div class="">
                                            <%= f.text_field :name,  class: "col-xs-12 balloon"%><label>名前</label>
                                        </div>
                                    </div>
                                    <div class="col-md-6 form_text">
                                        <div class="">
                                            <%=f.text_field :nick_name , class: "col-xs-12 balloon"%><label>ユーザー名</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6 form_text">
                                        <div class="">
                                            <%=f.email_field :email , class: "col-xs-12 balloon"%><label>メールアドレス</label>
                                        </div>
                                    </div>
                                    <div class="col-md-6 form_text">
                                        <div class="">
                                            <%=f.telephone_field :phone , class: "col-xs-12 balloon"%><label>電話番号</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="">
                                    <label class="control-label">お問い合わせ</label>
                                    <%=f.text_field :message, class: "index_contact_form" %>
                                </div>
                                <div class="row">
                                    <div class="col-md-12 form_button">
                                        <%= f.submit "送信" ,class: "btn btn-default pull-right col-xs-6" %>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        <% end %>
    </div>
