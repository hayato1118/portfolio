<script>
AOS.init({
offset: 200,
duration: 600,
easing: 'ease-in-sine',
delay: 100,
});
</script>

<script>
  $(function() {
  $('.accordion-item').click(function(){

    var $answer = $(this).children('.answer');

    if ($answer.hasClass('open')) {

      $answer.removeClass('open');

      $answer.slideUp();

      $(this).children('span').text('+');

    } else {

      $answer.addClass('open');

      $answer.slideDown();

      $(this).children('span').text('-');
    }
  });
});
</script>
